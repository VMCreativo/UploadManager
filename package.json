{
  "name": "vmcreativo/upload-manager",
  "version": "0.0.1",
  "description": "Laravel Package to handle file uploads",
  "keywords": ["Laravel","file","upload","image","manager","directory"],
  "license": "MIT",
  "authors": [
    {
      "name": "Cristian Deluxe",
      "email": "cristian@vmcreativo.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "intervention/image": "^2.4",
  },
  "require-dev": {
    "phpunit/phpunit": "4.0.*"
  },
  "autoload": {
    "psr-4": {
      "VMCreativo\\UploadManager\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "VMCreativo\\UploadManager\\Providers\\UploadServiceProvider"
      ],
      "aliases": {
        "UploadFile": "VMCreativo\\UploadManager\\Facades\\UploadFile"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}